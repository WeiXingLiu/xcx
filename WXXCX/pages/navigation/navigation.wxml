<view>
  <text>the options value is {{options.options}}</text>
  <view wx:key="{{index}}" wx:for="{{[1,2,3,4,5,6,7,8,9]}}" wx:for-item="i">
    <view wx:key="{{index}}" wx:for="{{[1,2,3,4,5,6,7,8,9]}}" wx:for-item="j">
      <text wx:if="{{i <= j}}" style="display: inline-block;">
      {{i}} * {{j}} = {{i * j}}
      </text>
    </view>
  </view>
</view>