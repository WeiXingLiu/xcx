    justify-content: space-between;
}

.itemContainer {
    position: relative;
    margin-bottom: 40rpx;
    width: 120rpx;
    height: 120rpx;
}

.image {
    width: 100%;
    height: 100%;
}

.delete {
    position: absolute;
    top: 0;
    right: 0;
    width: 40rpx;
    height: 40rpx;
}