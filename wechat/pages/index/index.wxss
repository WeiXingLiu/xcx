
.mapContainer {
    width: 100%;
    /* height: 90% */
    position: absolute;
    top: 0; 
    bottom: 60px;
}

.mapLocationContainer {
    position: relative;

    width: 100%;
    height: 100%
}

.currentLocation {
    position: absolute;
    width: 20px;
    height: 40px;
    top: 50%;
    left: 50%;
    margin-left: -10px;
    margin-top: -40px;
}

.btnContainer {
    position: absolute;
    bottom: 60px;
    margin-left: 20px;
    width: 30px;
    height: 150px;
}

.clickBtn {
    background-color: darkgray;
    border-radius: 20px;
    width: 30px;
    height: 30px;
    margin-top: 20px;
}

.scanBtn {
    width: 100%;
    height: 60px;
    bottom: 0;
    position: fixed;

    line-height: 60px;
    background-color: brown;
    color: black;
    border-radius: 0;
    justify-content: center;
    align-content: center
}
